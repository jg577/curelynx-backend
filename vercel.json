{
    "version": 2,
    "builds": [
        {
            "src": "app/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        },
        {
            "src": "/api/.*",
            "dest": "/api/app.py"
        }
    ]
}